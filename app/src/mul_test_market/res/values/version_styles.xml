<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="tv_default">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">end</item>
        <item name="android:textSize">@dimen/default_text_size</item>
        <item name="android:textColor">@color/publish_select_zhg_color</item>
    </style>

    <style name="tv_default_white" parent="tv_default">
        <item name="android:textColor">@android:color/white</item>
    </style>
    <style name="login_input">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:textSize">16sp</item>
        <item name="android:paddingLeft">18dp</item>
        <item name="android:textColor">@color/pwk_text_color</item>

    </style>
</resources>